msgid "Single/never married"
msgstr "Ga ke a nyalwa/nyala/ ga keise ke ko ke nyalwe/nyale"

#: bcpp/choices.py:193
msgid "Married (common law/civil or customary/traditional)"
msgstr ""
"Ke nyetswe/nyetse (kwa ga molaodi kgotsa ka setswana le ko kgotleng tota)"

#: bcpp/choices.py:194
msgid "Divorced or formally separated"
msgstr "Ke kgaogane le mosadi/monna ka fa molaong"

#: bcpp/choices.py:195
msgid "Widowed"
msgstr "Ke swetswe"

#: bcpp/choices.py:202
msgid "Non formal"
msgstr "Thuto ga e golelwe"

