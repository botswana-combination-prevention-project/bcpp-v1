msgid "Family/friends did not want me to get an HIV test"
msgstr "Ba lelwapa/masika ba ne ba sa batle ke itlhatlhobela HIV"

#: bcpp/choices.py:225
msgid "I didn't have time due to work"
msgstr "Ke ne ke sena nako ka lebaka la tiro"

#: bcpp/choices.py:226
msgid "I didn't have time due to family obligations"
msgstr "Ke ne ke sena nako ka lebaka la ditshwarego tsa lelwapa"

#: bcpp/choices.py:227
msgid "My sexual partner did not want me to get an HIV test"
msgstr ""
"Mokapelo wa me wa tlhakanelo dikobo o ne a sa batle ke itlhatlhobela HIV"

#: bcpp/choices.py:236
msgid "No result recorded"
msgstr "Maduo ga a kwalwa"

#: bcpp/choices.py:240
msgid "In the last month"
msgstr "Mo lebakeng la kgwedi le le fetileng"

#: bcpp/choices.py:241
msgid "1 to 5 months ago"
msgstr "Kgwedi e le nngwe go ya ko go tse tlhano tse di fetileng"

#: bcpp/choices.py:242
