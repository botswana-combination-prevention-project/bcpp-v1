		#Redirect / http://192.168.1.65/dom_offline.html?

        ServerAdmin webmaster@localhost

        ErrorLog /var/log/apache2/django-bcpp-error.log

        LogLevel warn

        CustomLog /var/log/apache2/django-bcpp-access.log combined

        Alias /media/ /home/django_dev/source/bhp066/live/bhp066_project/bhp066/media/
        <Directory "/home/django_dev/source/bhp066/live/bhp066_project/bhp066/media/" >
          Order deny,allow
          Allow from all
        </Directory>

        Alias /sitestatic/ /home/django_dev/source/bhp066/live/bhp066_project/bhp066/sitestatic/
        <Directory "/home/django_dev/source/bhp066/live/bhp066_project/bhp066/sitestatic/" >
          Order deny,allow
          Allow from all
        </Directory>

        Alias /static/ /home/django_dev/source/bhp066/live/bhp066_project/bhp066/static/
        <Directory "/home/django_dev/source/bhp066/live/bhp066_project/bhp066/static/" >
          Order deny,allow
          Allow from all
        </Directory>

        WSGIScriptAlias / /home/django_dev/source/bhp066/live/bhp066_project/apache/django.wsgi

        <Directory "/home/django_dev/source/bhp066/live/bhp066_project/apache/" >
          Order deny,allow
